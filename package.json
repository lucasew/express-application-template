{
  "name": "express-application-template",
  "version": "0.0.1",
  "description": "A template to use for writing express applications",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run"
  },
  "repository": "github.com/lucasew/express-application-template",
  "author": "lucasew <lucas59356@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@types/hapi__joi": "^17.1.2",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "getenv": "^1.0.0",
    "knex": "^0.21.1",
    "morgan": "^1.10.0",
    "sqlite3": "^4.2.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/getenv": "^1.0.0",
    "@types/morgan": "^1.9.0",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
